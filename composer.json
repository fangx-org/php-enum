{
  "name": "fangx/php-enum",
  "description": "php enum generator",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "enum"
  ],
  "authors": [
    {
      "name": "nfangxu",
      "email": "nfangxu@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Fangx\\Enum\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fangx\\Tests\\": "tests/"
    }
  },
  "bin": [
    "bin/enum"
  ],
  "scripts": {
    "cs-fix": "php-cs-fixer fix $1",
    "test": "./vendor/phpunit/phpunit/phpunit --configuration phpunit.xml"
  }
}
